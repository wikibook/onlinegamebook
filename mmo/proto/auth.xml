<?xml version="1.0" encoding="utf-8"?>
<gen>
  <protocol name="auth_proto">
    <config source="authproto.cpp" header="authproto.h" debuglog="true" />


    <!--ping-->
    <method methname="ping">
      <param prtype="string" prname="data" prlength="50"/>
    </method>
	
    <!-- getAccount : AUTHSVに対してアカウント情報を要求する -->
    <method methname="getAccount">
      <param prtype="string" prname="name" prlength="30" />
    </method>
    <method methname="getAccountResult">
      <param prtype="string" prname="name" prlength="30" />
      <param prtype="string" prname="password" prlength="30" />	  
    </method>	


  </protocol>
</gen>
	


